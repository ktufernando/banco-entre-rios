    "/v1/ber/pymes/validateCuit/:cuit" : {
      "post" : {
        "tags" : [ "API - Endpoints" ],
        "description" : "Url - http://localhost:3000/v1/ber/pymes/validateCuit/:cuit - Endpoint para validar Cuit(pyme) Cuits de prieba - sin cuit - cuit incompleto - \"11111111111\" - \"22222222222\"",
        "parameters" : [ {
          "name" : "cuit",
          "in" : "query",
          "description" : "Content-Type:JSON",
          "required" : true,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "object",
            "example" : {
              "cuit" : "20734322237"
            }
          },
          "example" : "44444444444"
        } ],
        "responses" : {
          "200" : {
            "description" : "Cuit igual a \"33333333333\"",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object",
                  "description" : "Http status code response 200 mensaje OK",
                  "example" : {
                    "error" : false,
                    "message" : "OK",
                    "status" : 200,
                    "payload" : {
                      "isValid" : true,
                      "isMonotributista" : false
                    }
                  }
                }
              }
            }
          },
          "400" : {
