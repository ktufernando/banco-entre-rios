    "/pymes/validateCuit/{cuit}" : {
      "post" : {
        "tags" : [ "Pyme" ],
        "description" : "Endpoint para validar Cuit(pyme). Casos de prueba: Cuit menor o mayor a 11 caracteres > CUIT Invalido, Cuit igual a \"11111111111\" > CUIT no valido como empresa, Cuit igual a \"22222222222\" > Servicio del grupo no disponible, Cuit igual a \"33333333333\" > OK: isMonotributista: true, Cualquier otro caso > OK: isMonotributista: false",
        "parameters" : [ {
          "name" : "cuit",
          "in" : "path",
          "type":"string",
          "description" : "Cuit del cliente",
          "required" : true,
          "example" : "44444444444"
        } ],
        "responses" : {
          "200" : {
            "description" : "Cuit igual a \"33333333333\"",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object",
                  "description" : "Http status code response 200 mensaje OK",
                  "example" : {
                    "error" : false,
                    "message" : "OK",
                    "status" : 200,
                    "payload" : {
                      "isValid" : true,
                      "isMonotributista" : false
                    }
                  }
                }
              }
            }
          },
          "400" : {
            "description" : "Cuit menor o mayor a 11 caracteres",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object",
                  "description" : "Mensaje \"El CUIT es invalido\"",
                  "example" : {
                    "error" : {
                      "message" : "El Cuit es invalido"
                    },
                    "payload" : {
                      "isValid" : false,
                      "isMonotributista" : false
                    }
                  }
                }
              }
            }
          },
          "402" : {
            "description" : "Cuit igual a \"11111111111\"",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object",
                  "description" : "Mensaje \"CUIT no valido como empresa\"",
                  "example" : {
                    "error" : {
                      "message" : "CUIT no valido como empresa"
                    },
                    "payload" : {
                      "isValid" : false,
                      "isMonotributista" : false
                    }
                  }
                }
              }
            }
          },
          "504" : {
            "description" : "Cuit igual a \"22222222222\"",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object",
                  "description" : "Mensaje \"Servicio del grupo no disponible\"",
                  "example" : {
                    "error" : {
                      "message" : "Servicio del grupo no disponible"
                    },
                    "payload" : {
                      "isValid" : false,
                      "isMonotributista" : false
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
